speedConfirmation.TextScaled = true
speedConfirmation.TextSize = 16.000
speedConfirmation.TextWrapped = true
jumpPowerConfirmation.Parent = GeneralFrame
jumpPowerConfirmation.BackgroundColor3 = Color3.fromRGB(0, 85, 0)
jumpPowerConfirmation.BorderColor3 = Color3.fromRGB(0, 0, 0)
jumpPowerConfirmation.BorderSizePixel = 0
jumpPowerConfirmation.Position = UDim2.new(0.711, 0, 0.558, 0)
jumpPowerConfirmation.Size = UDim2.new(0, 120, 0, 50)
jumpPowerConfirmation.Font = Enum.Font.SourceSans

jumpPowerConfirmation.Text = "Confirm JumpPower"
jumpPowerConfirmation.TextColor3 = Color3.fromRGB(0, 170, 0)
jumpPowerConfirmation.TextScaled = true
jumpPowerConfirmation.TextSize = 16.000
jumpPowerConfirmation.TextWrapped = true


XButton.MouseButton1Click:Connect(function()
        GeneralFrame.Visible = false
end)
XButton.MouseButton1Click:Connect(function()
        GeneralFrame.Visible = false
end)
