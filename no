GodMode.MouseButton1Click:Connect(function()
        local player = game.Players.LocalPlayer
        local humanoid = player.Character:FindFirstChildWhichIsA("Humanoid")
        humanoid.Health = humanoid.MaxHealth
        humanoid:GetPropertyChangedSignal("Health"):Connect(function()
                if humanoid.Health < humanoid.MaxHealth then
                        humanoid.Health = humanoid.MaxHealth
                end
        end)
end)
speedConfirmation.MouseButton1Click:Connect(function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = TextBox.Text
end)
jumpPowerConfirmation.MouseButton1Click:Connect(function()
if game.Players.LocalPlayer.Character.Humanoid.UseJumpPower == true then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = TextBox.Text
else
        game.Players.LocalPlayer.Character.Humanoid.JumpHeight = TextBox.Text / 6.9444444444
        end
        end)
ESP.MouseButton1Click:Connect(function()
        local esp = Instance.new("Highlight")
        for _, player in game.Players:GetPlayers() do
                if player ~= game.Players.LocalPlayer then
                        esp:Clone().Parent = player.Character
                end
        end
end)
InfiniteYield.MouseButton1Click:Connect(function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/DarkNetworks/Infinite-Yield/main/latest.lua'))()
end)
