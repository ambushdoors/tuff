local player = game.Players.LocalPlayer
player.CharacterAdded:Connect(function()
        ScreenGui:Clone().Parent = player.PlayerGui
        TextButton:Clone().Parent = ScreenGui
        DOORSFrame:Clone().Parent = TextButton
        GeneralFrame:Clone().Parent = TextButton
        UtilitiesFrame:Clone().Parent = TextButton
        TextBox:Clone().Parent = GeneralFrame
        GeneralCategory:Clone().Parent = GeneralFrame
        DOORSCategory:Clone().Parent = GeneralFrame
        TextBox:Clone().Parent = GeneralFrame
        UtilitiesCategory_3:Clone().Parent = UtilitiesFrame
        UtilitiesCategory:Clone().Parent = GeneralFrame
        UtilitiesCategory_2:Clone().Parent = DOORSFrame
        DOORSCategory_3:Clone().Parent = UtilitiesFrame
        DOORSCategory_2:Clone().Parent = DOORSFrame
        GeneralCategory_3:Clone().Parent = UtilitiesFrame
        GeneralCategory_2:Clone().Parent = DOORSFrame
        TextBox_2:Clone().Parent = GeneralFrame
        speedConfirmation:Clone().Parent = GeneralFrame
        jumpPowerConfirmation:Clone().Parent = GeneralFrame
        mspaint:Clone().Parent = UtilitiesFrame
        DOORSEndlessMode:Clone().Parent = DOORSFrame
        DOORSHardcoreMode:Clone().Parent = DOORSFrame
        DOORSCreepyMode:Clone().Parent = DOORSFrame
        DOORSMayhemMode:Clone().Parent = DOORSFrame
        DOORSImpossibleMode:Clone().Parent = DOORSFrame
        DOORSScriptMode:Clone().Parent = DOORSFrame
        BackdoorExecutor:Clone().Parent = UtilitiesFrame
        InfiniteYield:Clone().Parent = UtilitiesFrame
        GodMode:Clone().Parent = GeneralFrame
        ESP:Clone().Parent = GeneralFrame
        DOORS:Clone().Parent = DOORSFrame
        XButton:Clone().Parent = GeneralFrame
        XButton_3:Clone().Parent = UtilitiesFrame
        XButton_2:Clone().Parent = DOORSFrame
        DOORS_2:Clone().Parent = UtilitiesFrame
