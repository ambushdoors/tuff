DOORSHardcoreMode.MouseButton1Click:Connect(function()
        local message = Instance.new("Message", game.Players.LocalPlayer.PlayerGui)
        message.Text = "note from ambush: this mode is currently broken, however DripCapybara is eventually going to fix it, thanks for reading! - 194ajh (if this message keeps appearing even after the mode is fixed, please contact me and i will remove it)."
        task.wait(8)
        message:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/DripCapybara/Doors-Mode-Remakes/refs/heads/main/HardcoreFixed.lua"))()
end)
